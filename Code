 >>> record = ("asd", 50, 123.45, (12, 18, 2012))
>>> name, *_, (*_, year) = record
>>> name
'asd'
>>> year
2012
>>> records = [
	("foo", 1, 2),
	("bar", 'hello'),
	('foo', 3, 4),
	]
>>> def do_foo(x, y):
	print('foo', x, y)

	
>>> def do_bar(s):
	print('bar', s)
  
  
>>> for tag, *args in records:
	if tag == 'foo':
		do_foo(*args)
	elif tag == 'bar':
		do_bar(*args)

		
foo 1 2
bar hello
foo 3 4
>>> items = [1, 10, 7, 4, 5, 9]
>>> head, *tail = items
>>> head
1
>>> tail
[10, 7, 4, 5, 9]
>>> def sum(items):
	head, *tail = items
	return head + sum(tail) if tail else head

>>> sum(items)
36
